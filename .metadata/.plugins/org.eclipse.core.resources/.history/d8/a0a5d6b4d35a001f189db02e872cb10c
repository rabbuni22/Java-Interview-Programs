import java.util.Arrays;

public class Permu {

	public static void main(String[] args) {
		String str="abc";
		char ch[]=str.toCharArray();
		for(int i=0;i<ch.length;i++) {
			for(int j=0;j<ch.length;j++) {
				char temp=ch[i];
				ch[i]=ch[j];
				ch[j]=temp;
				printArray(ch);
			}
		}
	}

	private static void printArray(char[] ch) {
		for(int i=0;i<ch.length;i++) {
			System.out.print(ch[i]+" ");
			System.out.print();
			
		}
		
	} 
		


		
	}




