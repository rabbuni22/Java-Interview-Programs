import java.util.ArrayList;
import java.util.Arrays;

public class Client{
public static void main(String[] args) {
	int arr[]= {1,2,3,4,5};
    for(int a:arr) {
    	if(a==5) {
    		System.out.println(+a+"the value present");
    	}else
    		System.out.println(+a+"the value not present");
    }
	
}

}
