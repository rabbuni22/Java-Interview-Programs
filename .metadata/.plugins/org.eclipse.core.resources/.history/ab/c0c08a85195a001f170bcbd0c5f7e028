import java.util.Arrays;

public class Permu {

	public static void main(String[] args) {
		String str="abcd";
		char ch[]=str.toCharArray();
		for(int i=0;i<ch.length;i++) {
			char c=ch[i];{
				for(int j=0;j<ch.length;j++) {
					char temp=ch[j];
					ch[j]=c;
					c=temp;
				}
			}
			System.out.println(Arrays.toString(ch));
		}
		
	}



}
