import java.util.Arrays;

public class Permu {

	public static void main(String[] args) {
		String str="abc";
		char ch[]=str.toCharArray();//a b 
		for(int i=0;i<ch.length;i++) {
	         char fx=ch[i];	//a b	
				for( int j=0;j<ch.length;j++) {
					swap(fx,ch[j]);
					
				}
				System.out.println(str);
				
			}
		
		}

	private static void swap(char fx, char c) {
		char temp=fx;
		fx=c;
		c=temp;
		
	}
		
	}




