
public class Tuple {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int arr[]= {7,5,2,3,8};
for(int i=0;i<arr.length;i++) {
	for(int j=i+1;j<arr.length;j++) {
		for(int k=0;k<arr.length;k++) {
		if((arr[i]+arr[j])==arr[k]) {
			System.out.println(arr[i]+":"+arr[j]);
		}
	}
}
	}
	}

}
