
public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
     String s="god";
     char arr[]=s.toCharArray();
     char temp;
     for(int i=0;i<arr.length-1;i++) {
    	 for(int j=i+1;j<arr.length;j++)
    	 {
    	 temp = arr[i];
    	 arr[i]=arr[j];
    	 arr[j]=temp;
    	 for(char c:arr) {
    		 System.out.print(c);
    	 }
    	 System.out.println();
    	 }
     }
	}
}

	


