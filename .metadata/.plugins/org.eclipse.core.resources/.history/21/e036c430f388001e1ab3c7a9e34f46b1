import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;
import java.util.Map;
import java.util.function.Function;
import java.util.stream.Collectors;

public class Even {
	public static void main(String[] args) {
		String str="javatest";
		char[] ch=str.toCharArray();
		ArrayList<Integer> al=new ArrayList();
		al.add(2);
		al.add(3);
		al.add(4);
		al.add(5);
		al.add(6);
		List<Integer> Even= al.stream().filter(a->a%2==0).collect(Collectors.toList());
	System.out.println(Even);
	List<Integer> odd= al.stream().filter(a->a%2!=0).collect(Collectors.toList());
	System.out.println(odd);
	//frequency of string
	
	Map<char[],Long> map=Arrays.asList(ch).stream().collect(Collectors.groupingBy(Function.identity(),Collectors.counting()));
	System.out.println(map);
	}
	

}
